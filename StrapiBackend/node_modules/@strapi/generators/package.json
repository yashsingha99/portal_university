    "copyfiles": "2.4.1",
    "fs-extra": "10.0.0",
    "node-plop": "0.26.3",
    "plop": "2.7.6",
    "pluralize": "8.0.0"
  },
  "devDependencies": {
    "@strapi/pack-up": "4.22.1",
    "eslint-config-custom": "4.22.1",
