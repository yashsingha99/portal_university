{
  "name": "prettier-plugin-packagejson",
  "version": "2.4.5",
  "description": "Prettier package.json plugin to make the order of properties nice.",
  "keywords": [
    "package.json",
    "plugin",
    "prettier"
  ],
  "repository": "github:matzkoh/prettier-plugin-packagejson",
  "license": "MIT",
  "main": "lib",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*.js",
    "!**/*.snap",
    "!lib/**/*.test.js"
  ],
  "scripts": {
    "fix": "run-p fix:*",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier -c .",
    "test": "run-s lint test:*",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "sort-package-json": "2.5.1",
    "synckit": "0.8.5"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "jest": "29.6.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0"
  },
  "peerDependencies": {
    "prettier": ">= 1.16.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  }
}
